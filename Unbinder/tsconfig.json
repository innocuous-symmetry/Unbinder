{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ES5",
    "outDir": "wwwroot/js"
  },
  "include": [
    "wwwroot/ts/**/*"
  ],
  "exclude": [
    "node_modules",
    "wwwroot/js"
  ]
}
